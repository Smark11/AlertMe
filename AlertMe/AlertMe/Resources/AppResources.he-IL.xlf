          <source>Please enable your location services by going to Settings -&gt; Location -&gt; Turn on Location Services option.</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">אנא הפעל שירותי המיקום שלך על-ידי מעבר להגדרות -&gt; מיקום -&gt; הפעל אפשרות שירותי איכון.</target>
        </trans-unit>
        <trans-unit id="Resx/Purchase" translate="yes" xml:space="preserve">
          <source>Purchase</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">רכישה</target>
        </trans-unit>
        <trans-unit translate="yes" id="Resx/_KeyWord5" xml:space="preserve">
          <source>Spot</source>
          <target state="translated" state-qualifier="tm-suggestion">זרקור</target>
        </trans-unit>
        <trans-unit translate="yes" id="Resx/_KeyWord4" xml:space="preserve">
          <source>Locale</source>
          <target state="needs-review-translation" state-qualifier="tm-suggestion">אזור</target>
