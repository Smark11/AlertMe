          <source>Your Name</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Твоето име</target>
        </trans-unit>
        <trans-unit id="Resx/Options" translate="yes" xml:space="preserve">
          <source>AlertMe Options</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">AlertMe опции</target>
        </trans-unit>
