        </trans-unit>
        <trans-unit id="Resx/AppMenuItemOptions" translate="yes" xml:space="preserve">
          <source>Options</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Опции</target>
        </trans-unit>
        <trans-unit id="Resx/Contact1Name" translate="yes" xml:space="preserve">
          <source>Contact 1 Name</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">1 име на контакт</target>
        </trans-unit>
        <trans-unit id="Resx/Contact2Name" translate="yes" xml:space="preserve">
          <source>Contact 2 Name</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">Име контакт 2</target>
        </trans-unit>
