        </trans-unit>
        <trans-unit id="Resx/Purchase" translate="yes" xml:space="preserve">
          <source>Purchase</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">購買</target>
        </trans-unit>
        <trans-unit translate="yes" id="Resx/_KeyWord5" xml:space="preserve">
          <source>Spot</source>
          <target state="needs-review-translation" state-qualifier="mt-suggestion">現貨</target>
